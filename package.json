{
  "name": "cypress_bot",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rm -r -f node_modules dist server/dist cypress/videos",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "start:ui": "webpack serve --port 3000",
    "start:server:dev": "ts-node -r tsconfig-paths/register server/index.tsx --port 3000",
    "build:ui": "NODE_ENV=production webpack",
    "build:ui:dev": "webpack",
    "build:server": "tsc -p ./server && tsc-alias -p ./server/tsconfig.json",
    "build": "npm run build:ui && npm run build:server",
    "server": "node server/dist/server/index.js --port 3000",
    "server:dev": "npm run build:ui:dev && nodemon --watch 'src/**/*' -e ts,tsx --exec 'npm run build:ui:dev && npm run start:server:dev'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:cypress": "concurrently -k 'npm run server' 'npm run cypress:run'",
    "storybook": "start-storybook -s ./ -p 6006",
    "docker:build": "docker build . -t 'xtonyx/cypress_bot'",
    "docker:run": "docker run -p 3000:3000 --name cypress_bot xtonyx/cypress_bot",
    "docker:clean": "docker rm -f cypress_bot && docker image rm -f 'xtonyx/cypress_bot'",
    "docker:run_clean": "npm run docker:clean && npm run docker:build && npm run docker:run"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-viewport": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@types/core-js": "^2.5.5",
    "@types/express": "^4.17.13",
    "@types/jquery": "^3.5.9",
    "@types/mocha": "^9.0.0",
    "@types/raf": "^3.4.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^6.4.0",
    "css-loader": "^5.2.7",
    "cypress": "^9.1.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "html-webpack-plugin": "^5.5.0",
    "jquery": "^3.6.0",
    "mini-css-extract-plugin": "^2.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "sass": "^1.45.0",
    "sass-loader": "^10",
    "storybook": "^6.4.4",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.4.2",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "dependencies": {
    "core-js": "^3.19.1",
    "express": "^4.17.1",
    "raf": "^3.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3",
    "whatwg-fetch": "^3.6.2"
  }
}
